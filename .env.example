# Firebase Configuration
# Скопируйте этот файл в .env и заполните своими данными

# Firebase API ключи
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Wildberries API
VITE_WB_API_KEY=your-wildberries-api-key

# OpenAI API
VITE_OPENAI_API_KEY=your-openai-api-key

# Google Custom Search (для поиска)
VITE_GOOGLE_SEARCH_API_KEY=your-google-search-api-key
VITE_GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id

# Режим разработки
VITE_DEV_MODE=true

# URL для API (если используете собственный бэкенд)
VITE_API_URL=http://localhost:3000/api
